<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tencent.grobot"
    android:versionCode="300"
    android:versionName="3.1.1" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="26" />

    <application
        android:debuggable="true"
        android:hardwareAccelerated="true" >
        <meta-data
            android:name="XYGameId"
            android:value="{XY_GAME_ID}" />
        <!-- remoteconfig test url -->
        <!-- <meta-data android:name="GCloud.GCloudCore.RemoteConfigUrl" android:value="http://10.123.16.139" /> -->
        <activity
            android:name="com.tencent.grobot.GRobotActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:hardwareAccelerated="true"
            android:screenOrientation="sensorLandscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>